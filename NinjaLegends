local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ninja Legends - Caasi", "Synapse")

-- MAIN
local Main = Window:NewTab("Teleports")

local MainSection = Main:NewSection("Misc")
MainSection:NewButton("SHOP", "Teleports you to the SHOP", function()
    teleportTO(game:GetService("Workspace").areaTeleportParts.rankPurchaseTeleportPart.CFrame)
end)

local MainSection = Main:NewSection("Islands")
function teleportTO(placeCFrame)
    local plyr = game.Players.LocalPlayer
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = placeCFrame
    end
end

MainSection:NewButton("Enchanted Island", "Teleports you to the Enchanted Island", function()
    teleportTO(game:GetService("Workspace").areaTeleportParts.groundToEnchantedIsland.CFrame)
end)

MainSection:NewButton("Astral Island", "Teleports you to the Astreal Island", function()
    teleportTO(game:GetService("Workspace").areaTeleportParts.groundToAstralIsland.CFrame)
